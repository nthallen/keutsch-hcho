FCC {
  HBox { +-; Title: "Flow Control"; -+ };
  ""      Bypass        Zero         Span;
  Set:    (FC2_Set,6)  (FC0_Set,5)  (FC1_Set,6)  sccm;
  Flow:   (FC2_Flow,5) (FC0_Flow,6) (FC1_Flow,6) sccm;
  Status: (FC2_Stat,7) (FC0_Stat,7) (FC1_Stat,7);
  Valve1: (Vlv1_Stat,7) Valve2:     (Vlv2_Stat,7);
}

FCC2 {
  HBox { +-; Title: "Flow"; -+ };
  Sample: (SampleFlow,6) sccm;
  Purge:  (PurgeFlow,6)  sccm;
# P:      (OmegaP,6)     Torr;
}
